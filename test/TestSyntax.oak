function Main ( ProgramArguments: Array:<str> ) : i32
{
	
	ignore ProgramArguments;
	
	bind MyString: str = "Hello, world!";
	
	bind MyArray: Array : <u32> = [:u32; 0u32, 1_u32, 2, 3, 4, 5, 6 ];
	
	bind comp:u32 = 0;
	
	if ( comp <= 2 )
	{
		
		print ( "a" );
		
	}
	else if ( comp <= 4 )
	{
		
		print ( "b" );
		
	}
	else
	{
		
		print ( "c" );
		
	}
	
	return 0;
	
}

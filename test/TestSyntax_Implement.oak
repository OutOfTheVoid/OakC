
trait AddOp
{
	
	function op_add ( LHS: & Self, RHS: & Self ): Self
	
}

struct Test
{
	
	bind A:i32
	
}

namespace MyNS
{
	
	implement Test
	{
		
		public function set ( & self, A:i32 )
		{
			
			self.A = A;
			
		}
		
	}
	
	implement Test for AddOp:<Test, Test>
	{
		
		function op_add ( LHS: & Self, RHS: & Self ): Self
		{
			
			bind mut New:Self;
			
			New.set ( LHS.A + RHS.A );
			
			return New;
			
		}
		
	}
	
}



namespace std::operators
{
	
	//#[builtin]
	trait AddOp
	{
		
		function op_add ( LHS: & Self, RHS: & Self ): Self
		
	}
	
}

namespace MyNS
{
	
	struct Test
	{
		
		A:i32
		
	}
	
	implement Test
	{
		
		public function set ( & self, A:i32 )
		{
			
			self.A = A;
			
		}
		
	}
	
	implement Test
	{
		
		public function get ( & self ): i32
		{
			
			return self.A;
			
		}
		
	}
	
	implement Test for ::std::operators::AddOp
	{
		
		function op_add ( LHS: & Self, RHS: & Self ): Self
		{
			
			bind mut New:Self;
			
			New.set ( LHS.A + RHS.A );
			
			return New;
			
		}
		
	}
	
}
